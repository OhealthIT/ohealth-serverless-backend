{
  "name": "ohealth-serverless-backend",
  "version": "1.0.0",
  "main": "handler.js",
  "license": "MIT",
  "scripts": {
    "dev": "DEBUG=ON serverless offline start",
    "deploy": "serverless deploy",
    "format": "prettier --write **/*.ts",
    "start": "ts-node src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "test": "jest",
    "dev-init:db": "docker pull postgres && docker run --name ohealth-postgres-dev -e POSTGRES_PASSWORD=secret -p 5432:5432 -d postgres",
    "create-migrations": "yarn typeorm migration:generate ./database/migrations/migration -d ./database/data-source.ts",
    "run-migrations": "yarn typeorm migration:run -d ./database/data-source.ts",
    "migrate-db": "yarn create-migrations && yarn run-migrations"
  },
  "dependencies": {
    "@anatine/zod-openapi": "^0.4.0",
    "@middy/core": "^2.5.1",
    "@middy/http-cors": "^2.5.2",
    "@middy/http-error-handler": "^2.5.1",
    "@middy/http-header-normalizer": "^2.5.2",
    "@middy/http-json-body-parser": "^2.5.1",
    "@middy/http-response-serializer": "^2.5.1",
    "@middy/http-security-headers": "^2.5.2",
    "@sentry/node": "^6.15.0",
    "@sentry/tracing": "^6.15.0",
    "axios": "^0.23.0",
    "csvtojson": "^2.0.10",
    "dayjs": "^1.10.7",
    "dotenv": "^16.0.1",
    "expo-server-sdk": "^3.6.0",
    "firebase-admin": "^9.12.0",
    "flat": "^5.0.2",
    "i18n-js": "^3.8.0",
    "json2csv": "^5.0.6",
    "lodash": "^4.17.21",
    "mixpanel": "^0.13.0",
    "nanoid": "^3.1.28",
    "nanoid-dictionary": "^4.3.0",
    "pg": "^8.4.0",
    "qrcode": "^1.4.4",
    "reflect-metadata": "^0.1.13",
    "serverless-bundle": "^5.4.0",
    "serverless-http": "^2.7.0",
    "serverless-logger": "^1.2.0",
    "stripe": "^8.178.0",
    "typeorm": "0.3.7",
    "zod": "^3.9.5"
  },
  "devDependencies": {
    "@gqless/cli": "^2.0.16",
    "@graphql-codegen/cli": "^2.2.1",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-graphql-request": "^4.1.4",
    "@graphql-codegen/typescript-operations": "^2.1.6",
    "@serverless/typescript": "^2.59.0",
    "@types/aws-lambda": "^8.10.83",
    "@types/http-errors": "^1.8.1",
    "@types/jest": "^28.1.7",
    "@types/lodash": "^4.14.176",
    "@types/nanoid-dictionary": "^4.2.0",
    "@types/node": "^16.11.10",
    "graphql-generate": "^0.0.0",
    "jest": "^28.1.3",
    "js-yaml": "^4.1.0",
    "prettier": "^2.4.1",
    "serverless-offline": "^8.2.0",
    "serverless-plugin-typescript": "^2.1.0",
    "source-map-support": "^0.5.20",
    "ts-jest": "^28.0.8",
    "ts-node": "10.7.0",
    "typescript": "4.5.2",
    "watch": "^1.0.2"
  }
}
